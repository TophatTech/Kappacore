void new_game() {
  char textArray[] = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789 ";
  char userName[11];
  boolean showUnder = 0;
  byte i = 0;
  byte textInc=0;
  boolean textStep = 0;

  tft.fillScreen(0x5ACB);

  tft.setTextSize(1);
  tft.setCursor(2, 0);
  tft.setTextColor(ST7735_WHITE);
  tft.print("  NAME YOUR AIRLINE  ");
  
  tft.setTextSize(2);
  resetall:
  tft.fillRect(0,10,128,20,0x5ACB);
  for(i=0; i<=9; i++){
    tft.setCursor((12*i)+4, 16);
    tft.print(textArray[textInc]);
    textStep = 0;
    while(textStep == 0){
      readJoyRaw();
      Serial.println(joyX);
      readButtons();
      if(btnR==1){
        goto resetall;
      }
      
      if(joyX>750){
          userName[i+1]=textArray[textInc];
          textStep = 1;
      }

      
      if(joyY>712){
        if(textInc < (sizeof(textArray)-1)){textInc++;}
        tft.setCursor((12*i)+4, 16);
        tft.setTextSize(2);
        tft.setTextColor(0x5ACB);
        tft.print(textArray[textInc-1]);
        tft.setCursor((12*i)+4, 16);
        tft.setTextColor(ST7735_WHITE);
        tft.print(textArray[textInc]);
      }

      if(joyY<312){
        if(textInc > 0){textInc--;}
        tft.setCursor((12*i)+4, 16);
        tft.setTextSize(2);
        tft.setTextColor(0x5ACB);
        tft.print(textArray[textInc+1]);
        tft.setCursor((12*i)+4, 16);
        tft.setTextColor(ST7735_WHITE);
        tft.print(textArray[textInc]);
      }

      if(joyY>1000 && textStep == 0|| joyY<10 && textStep==0){
      delay(100);
      }else{
      delay(300); 
      }

    }
  }
  tft.setTextSize(1);
  tft.setCursor(2, 34);
  tft.setTextColor(ST7735_WHITE);
  tft.print("COLOR FOR ");
  for(i=1; i<=10; i++){
    tft.print(userName[i]);
  }



  /*draw color selection */
  static const uint16_t PROGMEM colors[8] = {0x0000, 0x001F, 0xF800, 0x07E0, 0x07FF, 0xF81F, 0xFFE0, 0xFFFF};
  for(i=0; i<=7; i++){
    tft.fillRect((i*15)+4,46,12,12,colors[i]);
  }
  i=0;
  while(btnL==0){
    readJoyRaw();
    if(joyX > 800){
      if(i<7){i++;}
      }
    if(joyX < 200){
      if(i>0){i--;}
      }
    tft.drawRect((i*15)+3,45,14,14,0xFFFF);
    tft.drawRect(((i-1)*15)+3,45,14,14,0x5ACB);
    tft.drawRect(((i+1)*15)+3,45,14,14,0x5ACB);
    delay(300);
    readButtons();
  }

  
}




