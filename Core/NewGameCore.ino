/* DRAW MAIN MENU SELECTION (NEW/LOAD GAME) */
 void main_menu_selector() {

  byte i = 0;         //Menu Escape Variable
  byte mSelect = 0;   //Menu selection variable
  byte mDelay = 100;
  tft.setFont(&FreeSerifBold9pt7b);
  /*Print Initial Menu*/
  tft.setTextSize(1);
  tft.setTextColor(0xFDE4);
  tft.setCursor(2, 155);
  tft.print("NEW");
  tft.setTextColor(ST7735_WHITE);
  tft.setCursor(54, 155);
  tft.print("LOAD");
  while (i == 0) {
    mDelay++;
    readJoyRaw();
    if (joyX < 250) {
      if (mSelect == 1 && mDelay > 15) {
        tft.setTextColor(0xFDE4);
        tft.setCursor(2, 155);
        tft.print("NEW");
        tft.setTextColor(ST7735_WHITE);
        tft.setCursor(54, 155);
        tft.print("LOAD");
        mSelect = 0;
        mDelay = 0;
      }
    }

    if (joyX > 750) {
      if (mSelect == 0 && mDelay > 15) {
        tft.setTextColor(ST7735_WHITE);
        tft.setCursor(2, 155);
        tft.print("NEW");
        tft.setTextColor(0xFDE4);
        tft.setCursor(54, 155);
        tft.print("LOAD");
        mSelect = 1;
        mDelay = 0;
      }
    }
    //main_menu_plane();
    readButtons();
    if (btnL == 1) {
      if (mSelect == 0) {
        i = 1;
        new_game_text();
        new_game();
      }
    }
    
  }
  tft.setFont();
}


